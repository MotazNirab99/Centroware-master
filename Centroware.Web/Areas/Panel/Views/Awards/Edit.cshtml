@model Centroware.Model.DTOs.AwardsDto.AwardsUpdateDto
@{ Layout = null; }

<form asp-action="Edit" method="post" class="ajaxForm" tname="#kt_datatable">
    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label asp-for="Count"></label>
                <input asp-for="Count" class="form-control" />
                <span asp-validation-for="Count" class="text-danger"></span>
            </div>
        </div>

    </div>

    <div class="modal-footer">
        <button type="submit" class="btn btn-primary font-weight-bold mr-2">Submit</button>
        <button type="button" data-dismiss="modal" class="btn btn-light-primary font-weight-bold">Cancel</button>
    </div>
</form>


<script>PageLoadActions();</script>
<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

